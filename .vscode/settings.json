{
    "git.ignoreLimitWarning": true,
    "files.exclude": {
        "**/.venv": true,
        "**/__pycache__": true,
        "**/.history": true,
        "**/SOS_Output": true,
        "**/SOS_Output_TEST": true,
        "**/_ARCHIVE*": true,
        "**/_BACKUP*": true,
        "**/Reports": true,
        "**/Master_Analytics": true,
        "**/Gov-Test-Docs": true,
        "**/tests_archive": true,
